<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="BLueAccent" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorAccent">#002AFF</item>
    </style>

    <style name="EditTextLoginColor" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/fragment_login_description_color</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/ratingBarColorInactive</item>
        <item name="colorControlActivated">@color/ratingBarColor</item>
    </style>

</resources>
